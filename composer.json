{
    "name": "z1lab/tikket.com",
    "type": "project",
    "description": "A event platform.",
    "keywords": [
        "event",
        "ticket"
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "akaunting/money": "^1.0",
        "arcanedev/log-viewer": "^4.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "6.*",
        "jacobbennett/laravel-http2serverpush": "^2.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "tightenco/ziggy": "^0.6.6",
        "z1lab/laravel-meta-tags": "^3.1",
        "z1lab/openid-client": "1.0.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-ide-helper": "v2.5.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:models -N"
        ]
    }
}
